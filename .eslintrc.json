{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "semi": "off",
      "indent": "warn",
      "spaced-comment": "warn",
      "object-curly-spacing": "warn",
      "no-trailing-spaces": "warn",
      "arrow-parens": "off",
      "keyword-spacing": "off",
      "quotes": "off",
      "no-shadow": "off",
      "object-curly-newline" : "off",
      "lines-between-class-members" : "off",
      "max-len":"off"
    }
}
